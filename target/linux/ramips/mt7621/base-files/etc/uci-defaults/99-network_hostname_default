uci -q batch << EOI

# set network.lan.device='lan'
# commit network


set wireless.@wifi-iface[0].ssid='npotelecom-wifi-2g'
set wireless.radio0.country='RU'
set wireless.radio0.cell_density='0'
set wireless.default_radio0.encryption='psk2'
set wireless.default_radio0.key='password'

set wireless.@wifi-iface[1].ssid='npotelecom-wifi-5g'
set wireless.radio1.country='RU'
set wireless.radio1.cell_density='0'
set wireless.default_radio1.encryption='psk2'
set wireless.default_radio1.key='password'

set wireless.@wifi-device[0].disabled='0'
set wireless.@wifi-device[1].disabled='0'

commit wireless

set system.@system[0].hostname='AP5000AX'

add system button
set system.@button[0].button="wps"
set system.@button[0].action="released"
set system.@button[0].handler="firstboot -y && reboot now"
set system.@button[0].min="5"
set system.@button[0].max="15"

commit system

EOI
